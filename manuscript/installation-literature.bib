	keywords = {\_tablet, offshore wind turbines, installation, benefit-risk assessment, health \&   safety, multi-criteria decision making, offshore lifting operations, Technique for Order of Preference by   Similarity to Ideal Solution (TOPSIS)},
	pages = {1175},

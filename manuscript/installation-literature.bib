	note = {tex.ids= renDevelopmentApplicationSimulator2018a},
	keywords = {\_tablet, Offshore installation, Blade installation, MATLAB and Simulink, Simulation model},
	pages = {380--395},
	file = {2018_Ren et al_Development and application of a simulator for offshore wind turbine blades.pdf:/home/san/Zotero/storage/SR879HNP/2018_Ren et al_Development and application of a simulator for offshore wind turbine blades.pdf:application/pdf;ren_et_al_2018_development_and_application_of_a_simulator_for_offshore_wind_turbine_blades.pdf:/home/san/Zotero/storage/MX3MKU82/ren_et_al_2018_development_and_application_of_a_simulator_for_offshore_wind_turbine_blades.pdf:application/pdf;ScienceDirect Snapshot:/home/san/Zotero/storage/2YAAREK3/S0029801818307224.html:text/html},

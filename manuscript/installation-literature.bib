@phdthesis{sehts_prognose_2021,
	address = {Bremen},
	type = {Bachelor of {Science}},
	title = {Prognose der {Installationskosten} von {Offshore}-{Windenergieparks} auf {Basis} von empirischen {Daten}},
	abstract = {To make the installation of offshore wind turbines more profitable, installation costs
must be minimized. This can be achieved, among other things, by reducing
installation times. This work aims to establish correlations between prevailing
environmental conditions and rotor blade installation to subsequently make
predictions for future installation projects. Furthermore, it is investigated to what
extent the use of a vibration damper changes the installation frequency in a year. For
this purpose, worldwide data measurements from 2007-2017 are examined. Using
the programming language "R", time windows are filtered out that have installationfriendly wind speeds and wave heights. Finally, the filtered datasets are examined for
contiguous times that reflect the respective installation times required with and
without vibration dampeners. It can be shown that the use of a vibration damper
noticeably increases the number of possible installation windows. The probability of a
successful installation is lowest in January and highest in June. Filtering wave height
and wind speed reduce the number of installation windows. The elaboration offers
future work the opportunity to build on the results and use them further to make
sense of the analysis of past installation projects using environmental data
forecasting.},
	school = {University of Bremen},
	author = {Sehts, Alexander},
	month = jan,
	year = {2021},
	keywords = {\_tablet},
	file = {Bachelorarbeit Alexander_Sehts_4435584-zusammengefügt.pdf:/home/san/Zotero/storage/5DX3PDDN/Bachelorarbeit Alexander_Sehts_4435584-zusammengefügt.pdf:application/pdf},
}

@article{stewart_offshore_2013,
	title = {Offshore {Wind} {Turbine} {Load} {Reduction} {Employing} {Optimal} {Passive} {Tuned} {Mass} {Damping} {Systems}},
	volume = {21},
	issn = {2374-0159},
	doi = {10.1109/TCST.2013.2260825},
	abstract = {Offshore wind turbines can capture the high-quality offshore wind resource but suffer from increased loading from waves and ice. Reducing these loads through structural control techniques has the potential to be an economically viable solution. Both fixed-bottom and floating substructures are considered in this paper, which will investigate a fixed-bottom monopile as well as a barge, spar buoy, and tension-leg platform for the floating platforms. A set of optimum passive tuned mass dampers are developed by creating a limited degree-of-freedom model for each of the four offshore wind platforms. These models are then integrated into an optimization function using a genetic algorithm to find a globally optimum design for the tuned mass damper. The tuned mass damper parameters determined by the optimization are applied to a series of wind turbine design code simulations using FAST. A sensitivity analysis of the tuned mass damper parameters and a study on the effect of misaligned wind and waves on load reductions are also conducted. Results from these simulations are presented, and tower fatigue damage reductions of up to 20\% are achieved for the various tuned mass damper configurations.},
	number = {4},
	journal = {IEEE Transactions on Control Systems Technology},
	author = {Stewart, Gordon and Lackner, Matthew},
	month = jul,
	year = {2013},
	keywords = {\_tablet, offshore installations, wind turbines, waves, offshore wind turbines, power engineering computing, damping, structural control, barge, control engineering computing, digital simulation, FAST, fixed-bottom monopile, fixed-bottom substructures, Floating offshore wind, floating platforms, floating substructures, genetic algorithm, genetic algorithms, high-quality offshore wind resource, ice, offshore wind turbine load reduction, optimal control, optimal passive tuned mass damping systems, optimization function, spar buoy, structural control techniques, tension-leg platform, tower fatigue damage reductions, tuned mass dampers, wind turbine design code simulations},
	pages = {1090--1104},
	file = {2013_Stewart_Lackner_Offshore Wind Turbine Load Reduction Employing Optimal Passive Tuned Mass.pdf:/home/san/Zotero/storage/5AA6TGD2/2013_Stewart_Lackner_Offshore Wind Turbine Load Reduction Employing Optimal Passive Tuned Mass.pdf:application/pdf;IEEE Xplore Abstract Record:/home/san/Zotero/storage/XRCEU7BT/6532422.html:text/html;Snapshot:/home/san/Zotero/storage/2ESDE9I9/6532422.html:text/html},

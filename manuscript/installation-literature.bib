	keywords = {Offshore installation, Allowable sea states, Limiting parameters, Marine operations, Operability, Operational limits, Weather windows},
	pages = {308--327},
	file = {guachamin_acero_et_al_2016_methodology_for_assessment_of_the_operational_limits_and_operability_of_marine.pdf:/home/san/Zotero/storage/BCKNS5VG/guachamin_acero_et_al_2016_methodology_for_assessment_of_the_operational_limits_and_operability_of_marine.pdf:application/pdf;guachamin_acero_et_al_2016_methodology_for_assessment_of_the_operational_limits_and_operability_of_marine.pdf:/home/san/Zotero/storage/6MVEHAM9/guachamin_acero_et_al_2016_methodology_for_assessment_of_the_operational_limits_and_operability_of_marine.pdf:application/pdf;ScienceDirect Snapshot:/home/san/Zotero/storage/N6KVP6MT/S0029801816303419.html:text/html},

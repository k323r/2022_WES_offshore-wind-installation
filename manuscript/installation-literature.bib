	year = {2016},
	note = {WOS:000387586600021},
	keywords = {wind turbine, offshore, installation, decision support, marine operations, probability, risk, weather window},
	pages = {207--217},
	file = {Gintautas et al. - 2016 - Towards a risk-based decision support for offshore.pdf:/home/san/Zotero/storage/HKUUQ9LH/Gintautas et al. - 2016 - Towards a risk-based decision support for offshore.pdf:application/pdf},
}

@incollection{matha_fabrication_2017,
